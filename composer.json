{
    "name": "serafim/opcache",
    "license": "MIT",
    "description": "PHP opcache decoder",
    "require": {
        "php": "~7.1",
        "ext-mbstring": "*",
        "ext-zend-opcache": "*",
        "symfony/finder": "~3.4|~4.0"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\Opcache\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "~3.4|~4.0",
        "phpunit/phpunit": "~7.0|~8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\Opcache\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
